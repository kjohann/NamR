@page "/"
@using NamR.Shared
@inject NavigationManager NavigationManager
@inject Blazored.LocalStorage.ILocalStorageService localStorage


@code {
    protected async override Task OnInitializedAsync()
    {
        var listId = await localStorage.GetItemAsync<Guid?>(Constants.ListIdLocalStorageKey) ?? Guid.NewGuid();
        NavigationManager.NavigateTo($"/list/{listId}");
    }
}
